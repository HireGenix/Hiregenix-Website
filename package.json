{
  "name": "hiregenix-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "NEXT_DISABLE_LINT=true next build",
    "start": "next start",
    "lint": "next lint",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:check": "node scripts/check-postgres.js",
    "create:admin": "node scripts/create-admin-user.js",
    "analyze": "ANALYZE=true next build",
    "type-check": "tsc --noEmit",
    "postinstall": "prisma generate",
    "prepare:production": "chmod +x scripts/check-postgres.js && npm run db:deploy && npm run db:seed"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.12",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.3",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.3",
    "@mui/material": "^5.15.3",
    "@prisma/adapter-pg": "^6.5.0",
    "@prisma/client": "^5.22.0",
    "@types/nodemailer": "^6.4.17",
    "@uiw/react-md-editor": "^4.0.5",
    "axios": "^1.6.3",
    "bcryptjs": "^2.4.3",
    "critters": "^0.0.23",
    "date-fns": "^3.0.6",
    "dotenv": "^16.4.7",
    "framer-motion": "^12.4.10",
    "mongodb": "^6.15.0",
    "next": "^15.2.3",
    "next-auth": "^4.24.5",
    "nodemailer": "^6.10.0",
    "pg": "^8.14.1",
    "react": "^18",
    "react-beautiful-dnd": "^13.1.1",
    "react-color": "^2.19.3",
    "react-dom": "^18",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.49.2",
    "react-markdown": "^10.1.0",
    "react-masonry-css": "^1.0.16",
    "redis": "^4.7.0",
    "slugify": "^1.6.6",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/react-color": "^3.0.10",
    "@types/react-dom": "^18",
    "@types/uuid": "^10.0.0",
    "eslint": "^8",
    "eslint-config-next": "^15.2.3",
    "prisma": "^5.7.1",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
